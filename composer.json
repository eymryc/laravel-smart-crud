{
    "name": "rouangni/laravel-smart-crud",
    "description": "Intelligent CRUD generator for Laravel that analyzes your database structure and generates complete API resources",
    "type": "library",
    "keywords": [
        "laravel",
        "crud",
        "generator",
        "api",
        "repository",
        "dto",
        "smart",
        "database"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Romaric OUANGNI",
            "email": "wangny.ouangni@gmail.com",
            "homepage": "https://votre-site.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "rouangni\\SmartCrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rouangni\\SmartCrud\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rouangni\\SmartCrud\\Providers\\SmartCrudServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}